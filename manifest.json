{
    "name": "BanglarBhumi PDF Downloader",
    "description": "Extract records as PDF from BanglarBhumi",
    "version": "1.0",
    "author": "Sayed Ahmed",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup/popup.html",
        "default_title": "BanglarBhumi PDF Downloader"
